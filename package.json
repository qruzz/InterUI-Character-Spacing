{
  "name": "interuicharacterspacing",
  "version": "1.0.0",
  "description": "Package to correct the character spacing for the InterUI font in Sketch",
  "license": "MIT",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "InterUICharacterSpacing",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Michael Nissen <m9ssen@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qruzz/InterUI-Character-Spacing.git"
  },
  "dependencies": {
    "npm": "^6.0.0"
  }
}
