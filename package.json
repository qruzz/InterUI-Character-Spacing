{
  "name": "interuicharacterspacing",
  "author": "Michael Nissen <m9ssen@gmail.com>",
  "version": "1.2.1",
  "description": "Package to correct the character spacing for the InterUI font in Sketch",
  "license": "MIT",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "InterUICharacterSpacing",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qruzz/InterUI-Character-Spacing.git"
  }
}
